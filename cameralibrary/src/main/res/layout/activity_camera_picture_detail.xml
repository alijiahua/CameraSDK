<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	tools:context=".Activity.Setting.OtherUserActivity">


	<include
		layout="@layout/new_title"
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:layout_heightPercent="8%h"
		app:layout_widthPercent="100%w"
		/>
	<com.zhy.android.percent.support.PercentRelativeLayout
		android:layout_width="match_parent"
		android:layout_height="fill_parent"
		>

		<ImageView
			android:id="@+id/id_img"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			/>


           <LinearLayout
			   android:layout_width="0dp"
			   android:layout_height="0dp"
			   app:layout_heightPercent="8%h"
			   app:layout_widthPercent="100%w"
			   android:layout_alignParentBottom="true"
			   android:orientation="horizontal"
			   android:gravity="center"
			   >
<ImageView
	android:id="@+id/id_share"
	android:layout_width="wrap_content"
	android:layout_height="match_parent"
	android:src="@drawable/scene_del"
	android:paddingLeft="25dp"
	android:paddingRight="25dp"
	/>
<ImageView
	android:id="@+id/id_del"
	android:layout_width="wrap_content"
	android:layout_height="match_parent"
	android:src="@drawable/set_share"
	android:paddingLeft="25dp"
	android:paddingRight="25dp"
	/>


		   </LinearLayout>

	</com.zhy.android.percent.support.PercentRelativeLayout>



</com.zhy.android.percent.support.PercentLinearLayout>
